{% extends "layout.html.twig" %}

{% block title %}

{% endblock %}

{% block description %}

{% endblock %}

{% block main_title %}

{% endblock %}

{% block main_body %}
    <turbo-frame id="table_crud" data-turbo-action="advance">
        <div class="card bg-base-100">
            <span class="m-3">{{ knp_pagination_sortable(pagination, "common.created_at"|trans, "s.createdAt", {}, {}, "knp_custom/paginator_sortable.html.twig") }}</span>
            <div class="card-body">
                {% for stack in pagination %}
                    {{ component("listItem", { item: stack }) }}
                {% else %}
                    {{ "common.no_result"|trans }}
                {% endfor %}
            </div>
        </div>
        <div class="mt-3 flex items-center justify-center">
            {{ knp_pagination_render(pagination, "knp_custom/paginator.html.twig") }}
        </div>
    </turbo-frame>
{% endblock %}
